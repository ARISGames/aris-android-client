<?xml version="1.0" encoding="utf-8"?>
<edu.uoregon.casls.aris_android.object_controllers.DialogOptionLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fffefefe"
                android:clickable="true"
                android:focusable="true"
                android:alpha=".9">

    <!-- Dialog Option HTML (text) -->
    <LinearLayout
        android:id="@+id/ll_dialog_option"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:alpha=".9"
        android:background="#fffefefe"
        android:layout_toLeftOf="@+id/iv_game_item_right_arrow"
        android:paddingTop="5dp"
        android:clickable="false"
        android:focusable="false"
        android:paddingBottom="5dp">
        <!--<WebView-->
        <view
            class="edu.uoregon.casls.aris_android.ARISWebView"
            android:id="@+id/awv_dialog_option"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:background="#fffefefe"
            android:clickable="false"
            android:focusable="false"
            android:alpha=".98"/>
    </LinearLayout>

    <!-- Right end arrow  -->
    <ImageView
        android:id="@+id/iv_game_item_right_arrow"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:src="@drawable/arrow_forward_2x"
        android:layout_alignParentBottom="false"
        android:paddingBottom="5dp"
        android:clickable="false"
        android:focusable="false"
        android:layout_marginBottom="5dp"/>


</edu.uoregon.casls.aris_android.object_controllers.DialogOptionLayout>
